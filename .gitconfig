[user]
  email = timocratic@gmail.com
  name = Tim Connor
[alias]
  co = checkout
  cm = commit -m
  all = commit -a -m
  rb = rebase -i origin
  st = status
  amend = commit --amend
  aa = commit -a --amend
  up = !source ~/.dotfiles/.bash_git && gitup
  dc = !source ~/.dotfiles/.bash_git && gitup && git push
  conflicts = !git ls-files --unmerged | cut -c51- | sort -u | xargs $EDITOR
  resolve = !git ls-files --unmerged | cut -c51- | sort -u | xargs git add
[branch "master"]
  rebase = true
  remote = origin
  merge = refs/heads/master
[branch]
  autosetuprebase = always
[push]
  default = current
