gitup() { 
  if git update-index --refresh; then
    stashed=
  else
    stashed=1
    git stash save 'update temp'
  fi
  git fetch;
  git rebase -v origin/master;
  if [ -n "$stashed" ]; then
     git stash pop
  fi
}
wtm(){
  export GIT_AUTHOR_NAME="Tim Martin"
  export GIT_AUTHOR_EMAIL="tim.martin@yieldbuild.com"
}
wotm(){
  unset GIT_AUTHOR_NAME
  unset GIT_AUTHOR_EMAIL
}